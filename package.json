{
  "name": "quill-table-up",
  "type": "module",
  "version": "0.1.0",
  "description": "A table module for quill2.x",
  "author": "zzxming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzxming/quill-table-up"
  },
  "keywords": [
    "quill",
    "table",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./table-creator.css": "./dist/table-creator.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "build": "gulp --require @esbuild-kit/cjs-loader",
    "dev": "gulp --require  @esbuild-kit/cjs-loader dev"
  },
  "peerDependencies": {
    "quill": "^2.0.0"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@zzxming/eslint-config": "^0.0.10",
    "autoprefixer": "^10.4.19",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-yml": "^1.14.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "postcss-pxtorem": "^6.1.0",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-svg-import": "^3.0.0",
    "typescript": "^5.5.3",
    "yaml-eslint-parser": "^1.2.3"
  }
